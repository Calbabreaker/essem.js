{
    "name": "essem.js",
    "version": "0.1.0",
    "description": "Javascript ecs game engine.",
    "main": "build/essem.js",
    "module": "build/essem.module.js",
    "types": "build/essem.d.ts",
    "files": [
        "build/",
        "README.md",
        "package.json",
        "LICENSE"
    ],
    "scripts": {
        "start": "pnpm run dev",
        "dev": "rollup -cw --environment SERVER",
        "build": "rollup -c",
        "lint": "eslint --ext .js,.ts . --max-warnings 0",
        "lint:fix": "pnpm run lint --fix",
        "docs": "webdoc --site-root docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Calbabreaker/essem.js.git"
    },
    "keywords": [
        "library",
        "javascript",
        "typescript",
        "application",
        "games",
        "engine",
        "ecs",
        "framework",
        "renderer"
    ],
    "author": "Calbabreaker",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Calbabreaker/essem.js/issues"
    },
    "homepage": "https://calbabreaker.github.io/essem.js/",
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/node": "^20.11.16",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@webdoc/cli": "^2.2.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "rollup": "^4.9.6",
        "rollup-plugin-dts": "^6.1.0",
        "rollup-plugin-glsl": "^1.3.0",
        "rollup-plugin-re": "^1.0.7",
        "rollup-plugin-serve": "^1.1.1",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
    }
}
