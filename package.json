{
  "name": "essem.js",
  "version": "0.0.0",
  "description": "A javascript based game engine that can make any fast and powerful application.",
  "main": "build/essem.js",
  "module": "build/essem.module.js",
  "types": "build/essem.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "rollup -cw --environment DEV,SERVER",
    "build": "rollup -c",
    "build:dev": "rollup -c --environment DEV",
    "add-build-changes": "git add --force build/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Calbabreaker/essem.js.git"
  },
  "keywords": [
    "library",
    "javascript",
    "typescript",
    "maker",
    "application",
    "games",
    "engine"
  ],
  "author": "Calbabreaker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Calbabreaker/essem.js/issues"
  },
  "homepage": "https://github.com/Calbabreaker/essem.js#readme",
  "devDependencies": {
    "rollup": "^2.37.1",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-jscc": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3"
  }
}