{
    "name": "essem.js",
    "version": "0.0.2",
    "description": "Javascript ecs game engine.",
    "main": "build/essem.js",
    "module": "build/essem.module.js",
    "types": "build/essem.d.ts",
    "files": [
        "build/",
        "README.md",
        "package.json",
        "LICENSE"
    ],
    "scripts": {
